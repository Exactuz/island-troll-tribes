library Map {
  public struct Map {
    private static trigger allInitFuncs = CreateTrigger();

    static method onGameStart( code f ) {
      TriggerAddAction( allInitFuncs, f );
    }

    static method fireStartGameEvents() {
      TriggerExecute( allInitFuncs );
      DestroyTrigger( allInitFuncs );
      allInitFuncs = null;
    }
  }
}
