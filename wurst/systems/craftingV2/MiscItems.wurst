    ..addBonusDamage(12)
    ..addBonusStrength(12)

    new CustomItemType(ITEM_MAMMOTH_BOOTS, ITEM_MAMMOTH_BOOTS_DEFINITION)
    ..addBonusDamage(10)
    ..addBonusStrength(8)
    ..addBonusIntelligence(8)
