package MathUtils

public function ceil(real x) returns real
    let xi = x.toInt()
    let xir = xi.toReal()
    return x - xir > 0 ? (xi + 1).toReal() : xir

public function floor(real x) returns int
    return x.toInt()

public function round(real x) returns real
    return (x + 0.5).toInt().toReal()
